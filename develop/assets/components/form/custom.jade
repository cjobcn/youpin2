extends ../_base/index
append stylesheets
  link(rel="stylesheet", href="../_base/index.css")
  link(rel="stylesheet", href="./custom.css")
  style.
    body { height: 1068px; }
append script
  //script(type="text/javascript", src="../../require.js", data-main="./custom.js")
append part
  header
    .header-logo
      img(src="../../../images/form-logo-fonts.png")
  content
    .container
      form.login
        .form-title
          p
            i.icon-form-title
            a(href="#") 用户注册
          .caret-arrow
        .form-item
          p.company 上海协骏企业管理有限公司
        .form-item
          input(type="text", name="email", value="", placeholder="请输入邮箱")
        .form-item
          input(type="text", name="phone", value="", placeholder="请输入手机号")
        .form-item
          input(type="text", name="qq", value="", placeholder="请输入QQ号")
        .form-item
          input(type="password", name="password", value="", placeholder="请输入密码")
        .form-item
          input(type="password", name="password2", value="", placeholder="请确认输入密码")
        .form-item
          input(type="text", name="name", value="", placeholder="请输入姓名(必填)")
        .form-item
          input(type="text", name="ename", value="", placeholder="请输入英文名(必填)")
        .form-item
          input(type="submit", value="注册")
        .caret-1
        .caret-2
