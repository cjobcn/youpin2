template#template-my-position-aside
  .my-position-aside
    .my-position-side-btns
      h2.my-position-publish 发布职位
      h2.my-position-delegate 委托职位
    .side-img-1
      a(href="{{img1.href}}")
        img(v-attr="src:img1.imgSrc")
    .side-img-2
      .side-img-item(v-repeat="item in items")
        .side-img-item-img
          a(href="#")
            img(v-attr="src:item.imgSrc")
        p.side-img-item-title
          a(href="{{item.href}}") {{item.title}}
        p.side-img-item-content
          a(href="{{item.href}}") {{item.content}}
