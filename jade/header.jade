#header
  .container
    #site-logo.fl
      //- a.img.img-logo(href="{{logo.href}}")
      a(href="{{logo.href}}")
        img(src="{{logo.src}}", width="{{logo.width}}", height="{{logo.height}}")
    #site-header-menu.fr
      ul.site-header-link-menu
        li(v-repeat="item in imgs")
          a(class="img img-header-menu {{item.className}}", href="{{item.href}}")
            i.img.img-info-new(v-if="item.isNew")